{
  "kind": "collectionType",
  "collectionName": "ac_assets",
  "info": {
    "singularName": "ac-asset",
    "pluralName": "ac-assets",
    "displayName": "ACAsset",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "ShortDescription": {
      "type": "text"
    },
    "ReleaseNote": {
      "type": "richtext"
    },
    "AssetFile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Contact": {
      "type": "email",
      "default": "your@email.address"
    },
    "FAQ": {
      "type": "richtext"
    },
    "RoadMap": {
      "type": "richtext"
    },
    "AuthRequired": {
      "type": "boolean"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "acn-internal",
        "external"
      ]
    }
  }
}
